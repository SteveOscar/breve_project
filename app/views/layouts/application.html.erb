<!DOCTYPE html>
<html>
<head>
  <title>BreveProject</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_user %>
    <p>Welcome <%= current_user.username %> </p>
    <%= link_to "Log Out", logout_path, method: :delete %>
  <% end %>  


<%= yield %>

</body>
</html>
