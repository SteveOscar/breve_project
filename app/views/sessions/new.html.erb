<div class="container">
  <h1>Login To Account</h1>

  <%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Log In", class: "waves-effect waves-light btn" %>
  <% end %>
</div>
